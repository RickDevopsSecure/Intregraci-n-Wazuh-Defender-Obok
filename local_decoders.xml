<decoder name="microsoft_defender">
    <prematch>.*Microsoft Defender.*</prematch>
</decoder>

<decoder name="microsoft_defender_details">
    <parent>microsoft_defender</parent>
    <regex>"Action":\s*"(.*?)",\s*"Description":\s*"(.*?)",\s*"Severity":\s*"(.*?)",\s*"DeviceName":\s*"(.*?)"</regex>
    <order>action, description, severity, device_name</order>
</decoder>
